# Matrix operations
A = np.array([[1, 2], [3, 4]])
B = np.array([[5, 6], [7, 8]])

print(f"\nMatrix A:\n{A}")
print(f"Matrix B:\n{B}")

# Matrix addition
print(f"\nA + B:\n{A + B}")

# Matrix multiplication (NOT element-wise)
print(f"\nMatrix multiplication A @ B:\n{A @ B}")
print(f"\n")
print(f"Alternative multiplication A.dot(B):\n{A.dot(B)}")
