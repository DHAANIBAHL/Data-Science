import numpy as np

vector = np.array([1, 2, 3, 4, 5])
print(f"Vector: {vector}")
print(f"Shape: {vector.shape}")
print(f"Dimension: {vector.ndim}D")

# Vector operations
v1 = np.array([1, 2, 3])
v2 = np.array([4, 5, 6])

print(f"\nv1: {v1}")
print(f"v2: {v2}")
print(f"Addition: {v1 + v2}")
print(f"Subtraction: {v1 - v2}")
print(f"Scalar multiplication (v1 * 3): {v1 * 3}")
